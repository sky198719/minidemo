.input-class{
  width:100%;
  height:100rpx;
  margin-bottom: 20rpx;
  display: flex;
  align-items: center;
  padding: 20rpx;
  box-sizing: border-box;
  border-radius: 20rpx;
  background:#f5f5f5;
}
.btn-class{
  margin-top: 50rpx;
}
.btn-class button{
  display: block;
  width:100%;
  height:100rpx;
  line-height:100rpx;
  border-radius: 50rpx;
  color:#fff;
  background:#188afa;
  text-align: center;
}
.btn{
  width:100%;
  height:80rpx;
  text-align: center;
  line-height: 80rpx;
  background: #188afa;
  color:#fff;
  margin-top:40rpx;
  border-radius: 10rpx;
}
.title{
  font-size: 40rpx;
  font-weight: bold;
}
.right-icon{
  width:48rpx;
  height:48rpx;
  display: inline-block;
  position: absolute;
  right:40rpx;
}