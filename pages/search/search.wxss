.search-page{
  padding:10rpx 20rpx;
  box-sizing: border-box;
}
.search-items{
  width:100%;
  border-radius: 10rpx;
  background: #fff;
  height:80rpx;
  line-height: 80rpx;
  padding:0 20rpx;
  font-size: 30rpx;
  display: flex;
  margin-top:20rpx;
  box-sizing: border-box;
  align-items: center;
}
.s-i-name{
  display: inline-block;
  width:250rpx;

}
.search-picker{
  width:100%;
  text-align: right;
  color:#999;
  padding-right:80rpx;
}
.search-btn{
  width:100%;
  background:blue;
  text-align: center;
}